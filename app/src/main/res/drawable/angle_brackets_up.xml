<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorMyTransparent"></solid>
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="60"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="60">
            <shape android:shape="line">
                <stroke
                    android:width="2dp"
                    android:color="@color/colorMyLine">
                </stroke>
            </shape>
        </rotate>
    </item>
</layer-list>